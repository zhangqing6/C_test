#include<stdio.h>
#define choose(x,y)(x>5&&y<5)
int main() {
	int a, b;
	printf("请输入两个数字（并用空格隔开）\n");
	scanf_s("%d%d", &a, &b);
	int sum = 0;
	if (sum=choose(a,b)){
		printf("请输入你想存入的数字\n");
		int num =0;
		scanf_s("%d",& num);
		int* p = &num;
		printf("%d", p);
	}
	else {
		printf("错误\n");

	}
	return 0;
}