#include<stdio.h>
int main() {
	int arr[] = { 1,2,3,4,5,6,7,8,9 };
	int k = 7;
	int left = 0;
	int right = sizeof(arr) / sizeof(arr[0]) - 1;
	while (left <= right) {
		int mid = (left + right) / 2;
		if (arr[mid] < 7) {
			left = mid + 1;
		}
		else if (arr[mid] > 7) {
			right = mid - 1;
		}
		else {
			printf("找到了,下标为\n%d",mid);
			break;
		}
	}
	if (left > right) {

		printf("找不到\n");

	}
	return 0;
}